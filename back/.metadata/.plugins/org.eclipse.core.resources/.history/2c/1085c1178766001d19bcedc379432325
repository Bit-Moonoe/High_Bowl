package com.bowlcome.demo.service;

import java.util.List;

import org.springframework.http.ResponseEntity;
import org.springframework.stereotype.Service;

import com.bowlcome.demo.Entity.UserEntity;

import lombok.extern.slf4j.Slf4j;



@Service
public interface UserService {
	
	List<UserEntity> findAllUsers();
	
	UserEntity findUserId(String id);
	
	UserEntity signUpUser(UserEntity.Requests newUser);
	
	UserEntity loginUser(UserEntity.Requests registeredUser);
	
	UserEntity findUserInfo(UserEntity.Requests user);

}
